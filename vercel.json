{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "AddisBackend/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true
}